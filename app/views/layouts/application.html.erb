    <meta name="viewport" content="width=device-width,initial-scale=1">
    <!-- HTML Meta Tags -->
    <meta name="description" content="Red Dot Ruby Conference 2024">

    <!-- Facebook Meta Tags -->
    <meta property="og:url" content="https://reddotrubyconf.com">
    <meta property="og:type" content="website">
    <meta property="og:title" content="RDRC 2024">
    <meta property="og:description" content="Red Dot Ruby Conference 2024">
